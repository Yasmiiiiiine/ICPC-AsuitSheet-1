#include <iostream>

using namespace std;

int main()
{
    int number;
    int number2;
    cin>>number>>number2;
    int sum = number+number2;
    int substraction = number-number2;
    int multiply = number * number2;

    cout<< number << "+" << number2 <<"="<< sum <<endl;
    cout<< number<< "*" << number2 << "="<< multiply <<endl;
    cout<< number << "-" << number2 << "=" << substraction <<endl;

    return 0;
}
